{% for attachment in field.value %}
<a href="#" class="ea-thumbnail" data-featherlight="#ea-lightbox-{{ attachment.id }}" data-featherlight-close-on-click="anywhere">
    {# the second parameter is the name of the property with the UploadableField annotation #}
    <img style="max-width: 200px;" src="{{ vich_uploader_asset(attachment, 'imageFile') }}">
</a>
<div id="ea-lightbox-{{ attachment.id }}" class="ea-lightbox">
    {# the second parameter is the name of the property with the UploadableField annotation #}
    <img src="{{ vich_uploader_asset(attachment, 'imageFile') }}">
</div>
{% endfor %}